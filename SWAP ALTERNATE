public class Solution {
    
    public static void swapAlternate(int arr[]) {
    	//Your code goes here
        int length = arr.length;
        for(int i = 0 ; i < arr.length  ; i +=2){
            int start = arr[i];
            arr[i] = arr[i+1];
            arr[i+1] = start ;
        }
    }
}
